generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @default(autoincrement()) @id
  email String  @unique
  name  String
  passwordHash String @map("password_hash")

//displaySettings
  @@map("users")
  
}

model Graph{
  id Int @default(autoincrement()) @id
  isDirected Boolean @map("is_directed")
  //created_by
}

